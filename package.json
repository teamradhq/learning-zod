    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@typescript-eslint/utils": "^7.17.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
